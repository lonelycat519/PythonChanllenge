import string

def main():  
  intab = 'abcdefghijklmnopqrstuvwxyz'
  outtab = 'cdefghijklmnopqrstuvwxyzab'
  trantab = string.maketrans(intab, outtab)

  my_string = raw_input('What\'s the string?\n')  
  #  1. Hint: g fmnc wms bgblr rpylqjyrc gr zw fylb. rfyrq ufyr amknsrcpq ypc dmp. bmgle gr gl zw fylb gq glcddgagclr ylb rfyr'q ufw rfgq rcvr gq qm jmle. sqgle qrpgle.kyicrpylq() gq pcamkkclbcb. lmu ynnjw ml rfc spj.
  #  2. Apply on the url! (map)
  print my_string.translate(trantab)

if __name__ == '__main__':
  main()
